<% layout('/layouts/boilerplate') %>
<h1>All Naturopaths</h1>
    
    
<% for (let naturopath of naturopaths) { %>
    <div class="card mb-3">
        <div class="row">
            <div class="col-md-4">
                <img class="img-fluid" alt="" src="<%=naturopath.image %>">
            </div>
            <div class="col-md-8">
                <div class="card-body">
                    <h5 class="card-title"><%=naturopath.name%></h5>
                    <p class="card-text"><%=naturopath.description%></p>
                    <p class="card-text">
                        <small class="text-muted"><%=naturopath.location%></small>
                    </p>
                    <a class="btn btn-primary" href="/naturopaths/<%=naturopath._id%>">View <%=naturopath.name%></a>    
                </div>
            </div>
        </div>
    </div>
<% }%>

